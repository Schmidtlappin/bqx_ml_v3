      "bqx_feature_formula": {
        "canonical": "bqx_{window} = ((close[T] - close[T-window]) / close[T-window]) * 100",
        "sql": "((close_idx - LAG(close_idx, window) OVER (ORDER BY interval_time)) / NULLIF(LAG(close_idx, window) OVER (ORDER BY interval_time), 0)) * 100",
